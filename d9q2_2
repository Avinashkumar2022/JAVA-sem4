import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
        Scanner sc=new Scanner(System.in);
        float colour1=sc.nextFloat();
        float colour2=sc.nextFloat();
        float colour3=sc.nextFloat();
        System.out.print((int) Math.ceil(colour1)+" "); 
        System.out.print((int) Math.ceil(colour2)+" ");
        System.out.print((int) Math.ceil(colour3));
        
    }
}
